        </dt>

        <dd>
            @Html.DisplayFor(model => model.ProgramAdi)
        </dd>
        <br>
        <dt>
            <span class="details_title">Adet</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Adet)
        </dd>
        <br>
        <dt>
            <span class="details_title">Firma Adı</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FirmaAdi)
