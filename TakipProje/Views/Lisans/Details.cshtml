@model TakipProje.Models.Lisans

@{
    ViewBag.Title = "Lisans Ayrıntı";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br>
<br>
<center><h2>Lisans Bilgileri Ayrıntılar</h2></center>

<div>
    <h4>Lisans Ayrıntıları</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
