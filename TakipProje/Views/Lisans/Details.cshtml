<p>
    

    

    
    <div class="col-sm-offset-1 col-md-10">
        <span style="float:left;">@Html.ActionLink("Güncelle", "Edit", new { id = Model.ID }, new { @class = "btn btn-success" })</span>
        &nbsp;&nbsp;&nbsp;<button type="button" class="btn btn-info" onClick="location.href='/LisansDetay/Index'" style="transition:900ms;">Tarihçe</button>



        <div id="btn btn-primary" style="float:right">
            <button type="button" class="btn btn-primary" onClick="location.href='/Lisans/Index'">Geri Dön</button>
        </div>
    </div>
</p>

