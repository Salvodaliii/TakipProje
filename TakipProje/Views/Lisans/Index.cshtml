@model IEnumerable<TakipProje.Models.Lisans>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            <span>Program Adı</span>
        </th>
        <th>
            <span>Sabit Telefon</span>
        </th>
        <th>
            <span>Mail</span>
        </th>
        <th>
            <span>Alım Tarihi</span>
        </th>
        <th>
            <span>Bitiş Tarihi</span>
        </th>
        <th>
            <span>Yenileme Tarihi</span>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProgramAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SabitTelefon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mail)
        </td>
        <td>
            @item.AlimTarihi.ToString("yyyy.MM.dd")
        </td>
        <td>
            @item.BitisTarihi.ToString("yyyy.MM.dd")
        </td>
        <td>
            @item.YenilemeTarihi.ToString("yyyy.MM.dd")
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
