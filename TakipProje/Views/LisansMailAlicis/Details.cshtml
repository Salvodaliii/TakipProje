@model TakipProje.Models.LisansMailAlici

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br>
<br>
<center><h2>Lisans Alıcı Mail Ayrıntılar</h2></center>

<div>
    <h4>Alıcı Ayrıntılar</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <span class="details_title">Alıcı Mail Adresi</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AliciMailAdresi)
        </dd>

    </dl>
</div>
<p>
    <div class="col-sm-offset-1 col-md-10">
        <span style="float:left;">@Html.ActionLink("Güncelle", "Edit", new { id = Model.ID }, new { @class = "btn btn-success" })</span>


        <button style="float:right" type="button" class="btn btn-primary" onClick="location.href='/LisansMailAlicis/Index'">Geri Dön</button>
    </div>
</p>
