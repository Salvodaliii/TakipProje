@model TakipProje.Models.Yedekleme

@{
    ViewBag.Title = "Yedekleme Ayrıntı";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br>
<br>
<center><h2>Yedekleme Bilgileri Ayrıntılar</h2></center>

<div>
    <h4>Yedekleme Ayrıntıları</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <span class="details_title">Yedekleme Periyodu</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.YedeklemePeriyodu) Günde Bir
        </dd>

        <dt>
            <span class="details_title">Yedekleme Plan Adı</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.YedeklemePlaniAdi)
        </dd>

        <dt>
            <span class="details_title">Durum</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Durum)
        </dd>

        <dt>
            <span class="details_title">Oluşturma Tarihi</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.OlusturmaTarihi)
        </dd>

        <dt>
            <span class="details_title">Son Yedekleme Tarihi</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SonYedeklemeTarihi)
        </dd>

        <dt>
            <span class="details_title">Kontrol Eden Personel</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.KontrolEdenPersonel)
        </dd>

    </dl>
</div>
<p>
    <div class="col-sm-offset-1 col-md-10">
        <span style="float:left;">@Html.ActionLink("Güncelle", "Edit", new { id = Model.ID }, new { @class = "btn btn-success" })</span>


            <button style="float:right" type="button" class="btn btn-primary" onClick="location.href='/Yedekleme/Index'">Geri Dön</button>
    </div>
</p>
