@using TakipProje.Models;
@model List<Kullanicilar>
@{
    ViewBag.Title = "Profil";
}
<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<link href="~/bootstrap-4.0.0/dist/css/bootstrap-style.css" rel="stylesheet" />
<!------ Include the above in your HEAD tag ---------->

<div class="emp-profile">
    <div class="row">
        <div class="col-md-6">
            <div class="profile-head">
                <h5 style="font-size:20px;">
                    Kullanıcı Bilgileri
                </h5>
            </div>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-md-8">
            <div class="tab-content profile-tab" id="myTabContent">
                <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                    @foreach (var x in Model)
                    {
                        <div class="row">

                            <div class="col-md-6">
                                <label># ID</label>
                            </div>
                            <div class="col-md-6">
                                <p>@x.ID</p>
                            </div>
                        </div>

                        <div class="row">

                            <div class="col-md-6">
                                <label># Ad Soyad</label>
                            </div>
                            <div class="col-md-6">
                                <p>@x.KullaniciAdSoyad</p>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <label># Kullanıcı Mail</label>
                            </div>
                            <div class="col-md-6">
                                <p>@x.KullaniciMail </p>
                            </div>
                        </div>

                        <div class="row">

                            <div class="col-md-6">
                                <label># Yetki</label>
                            </div>
                            <div class="col-md-6">
                                <p>@x.KullaniciYetki</p>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <label># Kullanıcı Şifre</label>
                            </div>
                            <div class="col-md-6">
                                <p>@x.KullaniciSifre</p>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
    <hr />
</div>